    "check": "autochecker 0.12.9 4.0 5.0 6",
    "check-0.12": "autochecker 0.12.6",
    "commit": "commit-wizard",
    "size": "t=\"$(npm pack .)\"; wc -c \"${t}\"; tar tvf \"${t}\"; rm \"${t}\";"
